  "scripts": {
    "start": "node src/index.js",
    "watch": "nodemon -w src src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dragory/modmailbot.git"
  },
